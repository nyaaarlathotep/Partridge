<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.nyaaar.partridgemngservice.mapper.FileMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.nyaaar.partridgemngservice.entity.File">
        <id column="ID" property="id" />
        <result column="ELE_ID" property="eleId" />
        <result column="NAME" property="name" />
        <result column="TYPE" property="type" />
        <result column="PATH" property="path" />
        <result column="IS_AVAILABLE" property="isAvailable" />
        <result column="CREATED_TIME" property="createdTime" />
        <result column="UPDATED_TIME" property="updatedTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ID, ELE_ID, NAME, TYPE, PATH, IS_AVAILABLE, CREATED_TIME, UPDATED_TIME
    </sql>

</mapper>
